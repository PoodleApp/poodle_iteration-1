{
  "name": "poodle",
  "version": "1.0.0",
  "description": "Experimental email client with social features",
  "scripts": {
    "start": "make && electron build",
    "test": "babel-node node_modules/.bin/_mocha --full-trace --recursive -R spec -C --check-leaks test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hallettj/poodle.git"
  },
  "keywords": [
    "email"
  ],
  "author": "Jesse Hallett <jesse@sitr.us>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hallettj/poodle/issues"
  },
  "homepage": "https://github.com/hallettj/poodle#readme",
  "babel": {
    "presets": [
      "react",
      "es2015-node5"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.6.4",
    "babel-plugin-transform-class-properties": "6.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015-node5": "1.1.2",
    "babel-preset-react": "6.5.0",
    "chai": "3.5.0",
    "chai-as-promised": "5.2.0",
    "chai-immutable": "1.5.3",
    "electron-prebuilt": "1.2.1",
    "flow-bin": "0.27.0",
    "mocha": "2.4.5"
  },
  "dependencies": {
    "arfe": "hallettj/arfe#master",
    "electron-safe-ipc": "0.6.1",
    "email-addresses": "~2.0.1",
    "googleapis": "~2.1.3",
    "imap": "~0.8.15",
    "kefir": "3.2.0",
    "keytar": "~3.0.0",
    "mailcomposer": "hallettj/mailcomposer#patched",
    "mailparser": "hallettj/mailparser#5a63d765154da78020fbb2a5b8f399a481e82967",
    "marked": "~0.3.5",
    "material-ui": "0.15.0",
    "mkdirp": "~0.5.1",
    "moment": "~2.10.6",
    "mori": "0.3.2",
    "node-fetch": "~1.3.2",
    "object-assign": "~4.0.1",
    "pouchdb": "~4.0.1",
    "randomstring": "~1.0.7",
    "raw-body": "~2.1.4",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "react-router-redux": "4.0.4",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "3.5.2",
    "redux-crud-store": "uniqueway/redux-crud-store#release/4.0.0",
    "redux-logger": "2.6.1",
    "redux-saga": "0.10.4",
    "repa": "~1.0.0",
    "safety-lens": "1.4.0",
    "string-hash": "~1.1.0",
    "uuid": "~2.0.1",
    "vocabs-as": "^0.12.0",
    "xoauth2": "~1.1.0"
  }
